# Firebase Configuration
# Get these values from your Firebase project settings
# iOS App > Project Settings > General Tab > Your Apps > iOS App
# Android App > Project Settings > General Tab > Your Apps > Android App

FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Android specific (from google-services.json)
FIREBASE_ANDROID_API_KEY=AIzaSyAndroidAPIKey123456789
FIREBASE_ANDROID_APP_ID=1:123456789012:android:abcdefghijklmnop

# iOS specific (from GoogleService-Info.plist)
FIREBASE_IOS_API_KEY=AIzaSyiOSAPIKey123456789
FIREBASE_IOS_APP_ID=1:123456789012:ios:abcdefghijklmnop
FIREBASE_IOS_CLIENT_ID=123456789012-abcdefghijklmnopqrstuvwxyz123456.apps.googleusercontent.com
FIREBASE_IOS_REVERSED_CLIENT_ID=com.googleusercontent.apps.123456789012-abcdefghijklmnopqrstuvwxyz123456
FIREBASE_IOS_BUNDLE_ID=com.gorai.whisperlang

GOOGLE_SIGN_IN_WEB_CLIENT_ID=123456789012-abcdefghijklmnopqrstuvwxyz123456.apps.googleusercontent.com